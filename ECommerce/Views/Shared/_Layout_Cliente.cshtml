    <style>
        :root {
            --c-principal: #b1ce69;
        }

        .card .card-header h5:after {
            content: "";
            background-color: #b1ce69 !important;
            position: absolute;
            left: -20px;
            top: 0;
            width: 4px;
            height: 20px;
        }
        .product-category {
            text-transform: uppercase;
            font-size: 10px;
            color: #8D99AE;
        }

        .product-widget .product-body .product-name {
            text-transform: uppercase;
            font-size: 12px;
        }

            .product-widget .product-body .product-name > a {
                font-weight: 700;
            }

        .right-top-bar a:hover {
            color: #0f6069;
        }

        .top-bar {
            height: 40px;
            background: #9E9E9E;
            background: -webkit-linear-gradient(to right, #3a7bd5, #00d2ff);
            background: linear-gradient(to right, #78a4b8, #b1cc69);
        }

        .topbar-mobile li {
            height: 40px;
            background: #9E9E9E;
            background: -webkit-linear-gradient(to right, #3a7bd5, #00d2ff);
            background: linear-gradient(to right, #78a4b8, #b1cc69);
        }

        .icon-header-noti::after {
            content: attr(data-notify);
            font-family: "Proxima Nova",-apple-system,"Helvetica Neue",Helvetica,Roboto,Arial,sans-serif;
            font-size: 12px;
            color: #fff;
            line-height: 15px;
            text-align: center;
            display: block;
            position: absolute;
            top: -7px;
            right: 0;
            min-width: 15px;
            height: 15px;
            padding: 0 3px;
            background-color: #afce68;
        }

        .bg6 {
            background-color: background: #43cea2;
            background: -webkit-linear-gradient(to right, #185a9d, #9C27B0);
            background: linear-gradient(to right, #77a4b5, #b0cd68);
        }

        .bg1 {
            background-color: #8BC34A;
        }

        .footer-links li i {
            margin-right: 15px;
            color: #8bc34a;
            width: 14px;
            text-align: center;
        }

        .product-btns > button:hover {
            background-color: #E4E7ED;
            color: #b0cd68;
            border-radius: 50%;
        }

        .hov-btn3:hover {
            border-color: #cbdd99 !important;
            background-color: #b0cd68 !important;
            color: #fff;
        }

        .btn-back-to-top {
            display: none;
            position: fixed;
            width: 40px;
            height: 38px;
            bottom: 0px;
            right: 40px;
            background-color: #b1ce69;
            opacity: 0.5;
            justify-content: center;
            align-items: center;
            z-index: 1000;
            cursor: pointer;
            transition: all 0.4s;
            -webkit-transition: all 0.4s;
            -o-transition: all 0.4s;
            -moz-transition: all 0.4s;
        }

            .btn-back-to-top:hover {
                opacity: 1;
                background-color: #638906;
            }

        .main-menu > li:hover > a {
            text-decoration: none;
            color: #b0cd68;
        }

        a:hover, a:focus {
            color: #b0cd68;
            text-decoration: none;
            outline: none;
            /* background-color: #f9f9f9; */
        }

        .hov-cl1:hover {
            color: #b0cd68;
        }


        /*TESTING*/
        .pcoded #styleSelector {
            min-height: 300px !important;
            height: calc(100vh - 56px) !important;
            border: 1px solid #e9e9e9 !important;
            background: #FFF !important;
            position: fixed !important;
            margin: 0 !important;
            padding: 20px !important;
            width: 245px !important;
            z-index: 98 !important;
            -webkit-transition: 0.5s !important;
            transition: 0.5s !important;
            left: -10px !important;
            -o-transition: 0.5s !important;
        }
        .pcoded .pcoded-navbar .pcoded-item > li > a > .pcoded-micon {
            font-size: 14px;
            padding: 4px;
            margin-right: 10px;
            color: black;
            border-radius: 4px;
            width: 30px;
            display: inline-block;
            height: 30px;
            text-align: center;
        }

        .main-menu .main-menu-header:before {
            content: "";
            width: 100%;
            height: 100%;
            position: absolute;
            top: 0;
            left: 0;
            z-index: 0;
            background: linear-gradient(to right, #79a4b6cc, #afcb6ac9);
        }
        .pcoded .pcoded-navbar[active-item-theme="theme1"] .pcoded-item > li.active > a {
            background: linear-gradient(to right, #78a4b8, #78a4b8);
            color: #fff !important;
        }

        .main-menu .main-menu-header:before {
            content: "";
            background: linear-gradient(to right, #79a4b6cc, #afcb6ac9)!important;
            width: 100%;
            height: 100%;
            position: absolute;
            top: 0;
            left: 0;
            z-index: 0;
        }

/*        Ajuste de logo pantalla*/
