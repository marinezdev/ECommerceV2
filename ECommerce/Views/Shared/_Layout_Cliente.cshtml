        function EliminarCarrito(Num) {

             var obj = {};
            obj['Id'] = Num;
            var jsonObject = {
                "Carro_Electronico": obj
            };

            $.ajax({
                url: "@Url.Action("Eliminar_Articulo", "Store")",
                type: "POST",
               data: JSON.stringify(jsonObject),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                error: function (Contenido) {
                    alert("Mandar Mensaje de error");
                },
                success: function (Contenido) {
                    MuestraCarrito();
                }
            });

             $.ajax({
                url: "@Url.Action("Carrito_Consultar", "Store")",
                type: "POST",
                data: '',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                error: function (response) {
