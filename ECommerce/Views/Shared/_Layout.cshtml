        function EliminarCarrito(Num) {

            var CarroElectronico = new Object();
            CarroElectronico.Id = Num;

            $.ajax({
                url: "@Url.Action("CarroElectronico_Eliminar_Articulo", "CarroElectronico")",
                type: "POST",
                data: JSON.stringify(CarroElectronico),
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                error: function (Contenido) {
                    alert("Mandar Mensaje de error");
                },
                success: function (Contenido) {
                    MuestraCarrito();
                }
            });

             $.ajax({
                url: "@Url.Action("CarroElectronico_Consultar_Total", "CarroElectronico")",
                type: "POST",
                data: '',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                error: function (response) {
                    $('#BtCartShowp').attr('data-notify', 0);
                },
                success: function (response) {
                    $('#BtCartShowp').attr('data-notify', response.Total);
                }
             });

        }

        function MuestraCarrito() {
            $.ajax({
                url: "@Url.Action("CarroElectronico_Seleccionar_Clave", "CarroElectronico")",
                type: "POST",
                data: '',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                error: function (Contenido) {
                    $('#BtCartShowp').attr('data-notify', 0);
                    $('#TotalText').text('$0.00');
                    $('#ContenidoCarrito').html('');
                },
                success: function (Contenido) {
                    console.log(Contenido);
                    $('#ContenidoCarrito').html('');
                    var ContenidoCarrito = '';
                    var precio = 0;
                        for (var a = 0; a < Contenido.length; a++)
                        {
                    ContenidoCarrito += "<li class='header-cart-item flex-w flex-t m-b-12'>" +
                                            "<div  class='header-cart-item-img' onclick='EliminarCarrito(" + Contenido[a].CarroElectronico.Id + ")'>" +
                                                "<br><img src='https://" + Contenido[a].ArticuloImagen.ImagenURL + "' alt=''>" +
                                            "</div>" +
                                            "<div class='header-cart-item-txt p-t-8'>" +
                                                "<p class='product-category'>" + Contenido[a].CatClasificacion.Nombre + "</p>" +
                                                "<a href='#' class='header-cart-item-name m-b-18 hov-cl1 trans-04'>" +
                                                    Contenido[a].ArticuloDetalle.Nombre +
                                                "</a>" +
                                                "<span class='header-cart-item-info'>" +
                                                    Contenido[a].CarroElectronico.Item + " x " + Contenido[a].PrecioPublicoText +
                                                "</span>" +
                                            "</div>" +
                                "</li>";

                            precio += Contenido[a].CarroElectronico.Item * Contenido[a].ArticuloDetalle.PrecioPublico;
                        }


                    $('#ContenidoCarrito').html(ContenidoCarrito);
                    $('#TotalText').text('$' + commaSeparateNumber(Number.parseFloat(precio).toFixed(2)));
                    $('#BtCartShowp').attr('data-notify', Contenido.length);
                    //console.log(response);
                }
            });
        }


        function commaSeparateNumber(val) { while (/(\d+)(\d{3})/.test(val.toString())) { val = val.toString().replace(/(\d+)(\d{3})/, '$1' + ',' + '$2'); } return val; }

