    <script>
        $(document).ready(function () {
            $('#timer2').hide(0);
            $('#timer2').css('display', 'none');

            $('#EnviarMensaeje2').click(function () {

                var timeSlide = 1000;
                $('#timer2').hide(0);
                $('#timer2').css('display', 'none');

                $( "#EnviarMensaeje2" ).attr("disabled", true);
                $('.box-success2,.box-error2').slideUp(timeSlide);

                setTimeout(function () {
                    if (valida2() == "ok") {
                        $('#timer2').fadeOut(300);
                        $('#alertBoxes2').html('<div class="box-error2"></div>');
                        $('.box-error2').hide(0).html("Enviado <img src='http://www.serviciosinmobiliariosg.com.mx/images/icons/ajax-loader.gif' width='20px' height='20px'/>");
                        $('.box-error2').slideDown(timeSlide);

                        var arreglo = [];
                        var obj = {};

                        obj['email'] = $('#email2').val();

                        arreglo.push(obj);

                        var jsonObject = {
                            "Email": arreglo
                        };

                        //console.log(jsonObject);

                        $.ajax({
                            url: "@Url.Action("LlamarJsonEmail", "Email")",
                            type: "POST",
                            data: JSON.stringify(jsonObject),
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            error: function (response) {
                                $('#timer2').fadeOut(300);
                                $('#alertBoxes2').html('<div class="box-error2"></div>');
                                $('.box-error2').hide(0).html('Ha ocurrido un error durante la ejecución');
                                $('.box-error2').slideDown(timeSlide);
                                grecaptcha.reload();
                                grecaptcha.reset();
                            },
                            success: function (response) {
                                if (response == "ok") {

                                    $('#alertBoxes2').html('<div class="box-success2"></div>');
                                    $('.box-success2').hide(0).html('Envío éxitoso.');
                                    $('.box-success2').slideDown(timeSlide);
                                    $( "#EnviarMensaeje2" ).prop( "disabled", false );
                                    window.setTimeout('location.reload()', 3000); //Reloads after three seconds

                                    //window.location.reload(true);

                                    //location.reload();

                                } else {
                                    $( "#EnviarMensaeje2" ).prop( "disabled", false );
                                    $('#timer2').fadeOut(300);
                                    $('#alertBoxes2').html('<div class="box-error2"></div>');
                                    $('.box-error2').hide(0).html(response);
                                    $('.box-error2').slideDown(timeSlide);
                                    grecaptcha.reload();
                                    grecaptcha.reset();

                                }
                            }
                        });

                    }
                    else {
                        $('#timer2').fadeOut(300);
                        $('#alertBoxes2').html('<div class="box-error2"></div>');
                        $('.box-error2').hide(0).html(valida2());
                        $('.box-error2').slideDown(timeSlide);
                        $("#EnviarMensaeje2").prop("disabled", false);

                        $.notify({
	                        icon: 'fa fa-paw',
	                        message: "Formulario incompleto ."
                        },{
                                type: 'danger',
                            allow_dismiss: false,
                            animate: {
		                        enter: 'animated rollIn',
		                        exit: 'animated rollOut'
	                        }
                        });

                    }
                }, timeSlide);
                return false;
            });

        });

        function valida2() {
            var respuesta = "";
            $('#email2').css("border", "1px solid #64C0FF");

            if ($('#email2').val().length > 0) {
                if (Correo2() == "ok") {
                    respuesta = "ok";
                }else{
                    $('#email2').css("border", "1px solid red");
                    respuesta = "Coloca tu email correctamente";
                }
            } else {
                $('#email2').css("border", "1px solid red");
                respuesta = "Coloca tu email";
            }

            return respuesta;
        };

