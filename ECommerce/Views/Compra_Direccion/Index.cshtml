
@{
    Layout = null;
}


<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>E-Commerce </title>
    <!--===============================================================================================-->
    <link rel="shortcut icon" href="~/assets/images/icoCSB.ico">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/bootstrap/css/bootstrap.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/fonts/font-awesome-4.7.0/css/font-awesome.min.css" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/fonts/linearicons-v1.0.0/icon-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/slick/slick.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/MagnificPopup/magnific-popup.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/perfect-scrollbar/perfect-scrollbar.css">
    <!--===============================================================================================-->
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed:400,300,700" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Glegoo" rel="stylesheet" />

    <link rel="stylesheet" type="text/css" href="~/Content/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/main.css">

    <!--===============================================================================================-->
    <style>
        .wrap-menu-desktop {
            position: fixed;
            z-index: 101;
            /* background-color: #e9ecefc2; */
            background-color: #ffffffe0;
            width: 100%;
            height: 84px;
            top: 0px;
            left: 0px;
            -webkit-transition: height 0.3s, background-color 0.3s;
            -o-transition: height 0.3s, background-color 0.3s;
            -moz-transition: height 0.3s, background-color 0.3s;
            transition: height 0.3s, background-color 0.3s;
        }

        .how-itemcart1 {
            width: 130px;
            position: relative;
            margin-right: 20px;
            cursor: pointer;
        }

        .bor14 {
            border-radius: 10px;
        }
        .how-itemcart1::after {
            content: '';
            font-family: Linearicons-Free;
            font-size: 16px;
            display: -webkit-box;
            display: -webkit-flex;
            display: -moz-box;
            display: -ms-flexbox;
            display: flex;
            justify-content: center;
            align-items: center;
            position: absolute;
            width: 100%;
            height: 100%;
            top: 0;
            left: 0;
            background-color: rgb(0 0 0 / 0%);
            color: #fff;
            transition: all 0.3s;
            -webkit-transition: all 0.3s;
            -o-transition: all 0.3s;
            -moz-transition: all 0.3s;
            opacity: 0;
        }

        /*.form-control {
            display: block;
            width: 100%;
            padding: 0.5rem 0.75rem;
            font-size: 1rem;
            line-height: 1.25;
            color: #495057;
            background-color: #fff;
            background-image: none;
            background-clip: padding-box;
            border: 1px solid rgba(0,0,0,.15);
            border-radius: 0.25rem;
            transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
            height:42px;
        }*/
        .bg1 {
            background-color: #b0ce68;
        }

        .hov-btn1:hover {
            border-color: #b0ce68;
            background-color: #77a4b5;
            color: #fff;
        }
        .footer-links li i {
            margin-right: 15px;
            color: #b0ce68;
            width: 14px;
            text-align: center;
        }

        @@media (max-width: 991px) {
            .estetic {
                display: none;
            }
            .estetic1 {
                display: block !important;
            }
        }


        @@media (max-width: 767px) {
            .form-check {
                position: relative;
                display: block;
                margin-bottom: 0.5rem;
                padding-left: 4%;
            }

            .BtnCompra{
                width:50% !important;
            }
            .BtnCompraEspacing {
                display: none;
            }


        }

        @@media (max-width: 538px) {
            .form-check {
                position: relative;
                display: block;
                margin-bottom: 0.5rem;
                padding-left: 10%;
            }

            .texto {
                font-size: 15px !important;
            }
        }


        @@media (max-width: 465px) {

            .formatopago {
                font-size: 14px;
            }
        }
    </style>

</head>
<body>
    <!-- Header -->
    <header>
        <!-- Header desktop -->
        <div class="container-menu-desktop">

            <div class="wrap-menu-desktop">
                <nav class="limiter-menu-desktop container">

                    <!-- Logo desktop -->
                    <a href="#" class="logo">
                        <img src="~/images/icons/CSB_Mexico.png" />
                    </a>

                    <!-- Menu desktop -->
                    <div class="menu-desktop">
                        <ul class="main-menu">
                        </ul>
                    </div>

                    <!-- Icon header -->
                    <div class="wrap-icon-header flex-w flex-r-m">
                        <a href="@Url.Action("Carro", "Store")" class="flex-c-m cl0 size-103 bg1 bor1 hov-btn2 p-lr-15 trans-04">
                            <i class="fa fa-reply" aria-hidden="true"></i>  &nbsp;&nbsp;  volver al carrito
                        </a>
                    </div>
                </nav>
            </div>
        </div>
    </header>


    <div class="top-content">
        <p class="estetic">
            <br />
            <br />
            <br />
        </p>
        <p class="estetic1" style="display:none;">
            <br />
        </p>
        <div class="container">
            <div class="row">
                <div class="col-lg-10 col-xl-7 m-lr-auto m-b-50">
                    <div class="m-l-25 m-r--38 m-lr-0-xl">
                        <div class=" bor15 p-t-18 p-b-15 p-lr-40 p-lr-15-sm" style="border-top: 1px solid #e6e6e6; border-radius: 5px;">
                            <div class="row">
                                <div class="col-md-12">
                                    <h5><strong style=" color: #222222;">1.- DATOS DE ENTREGA  </strong> </h5>
                                    <hr />
                                    <strong><label style="font-size: 16px; color: #222222;">Información del cliente</label></strong>
                                    <p>Te enviaremos una confirmación del pedido a tu correo electrónico </p>
                                    <br />
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="Cliente_Correo"><strong>* Correo electrónico: </strong></label>
                                        <input type="email" class="form-control" id="Cliente_Correo" placeholder="" onKeyUp="CorreofrM('Cliente_Correo')" onchange="ValidaInput('Cliente_Correo')">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="Cliente_Nombre"><strong>* Nombre (s): </strong></label>
                                        <input type="text" class="form-control" id="Cliente_Nombre" placeholder="" onchange="ValidaInput('Cliente_Nombre')">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="Cliente_ApellidoPaterno"><strong>* Apellido Paterno: </strong></label>
                                        <input type="text" class="form-control" id="Cliente_ApellidoPaterno" placeholder="" onchange="ValidaInput('Cliente_ApellidoPaterno')">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="Cliente_ApellidoMaterno"><strong>* Apellido Materno: </strong></label>
                                        <input type="text" class="form-control" id="Cliente_ApellidoMaterno" placeholder="" onchange="ValidaInput('Cliente_ApellidoMaterno')">
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label for="Cliente_Telefono"><strong>* Teléfono: </strong></label>
                                        <input type="text" class="form-control input-number" id="Cliente_Telefono" maxlength="12" placeholder="" onchange="ValidaInput('Cliente_Telefono')">
                                    </div>
                                </div>
                                <div class="col-md-2">
                                    <br />
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="RadiosClienteTipoTelefono" id="RadiosTelefon1" value="Fijo" checked>
                                        <label class="form-check-label" for="RadiosTelefon1">
                                            Fijo
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="RadiosClienteTipoTelefono" id="RadiosTelefon2" value="Celular">
                                        <label class="form-check-label" for="RadiosTelefon2">
                                            Celular
                                        </label>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <br />
                        <div class=" bor15 p-t-18 p-b-15 p-lr-40 p-lr-15-sm" style="border-top: 1px solid #e6e6e6; border-radius: 5px;">
                            <div class="row">
                                <div class="col-md-12">
                                    <h5><strong style=" color: #222222;">2.- DOMICILIO DE ENTREGA  </strong> </h5>
                                    <hr />
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="CP"><strong>* Código postal: </strong></label>
                                        <input type="text" class="form-control input-number" name="CP" id="CP" maxlength="6" placeholder="Código postal" onchange="CPBuscar(this.value)">
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="Cliente_Calle"><strong>* Calle: </strong></label>
                                        <input type="text" class="form-control" id="Cliente_Calle" placeholder="" onchange="ValidaInput('Cliente_Calle')">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="Cliente_NumExterior"><strong>* Número exterior: </strong></label>
                                        <input type="text" class="form-control" id="Cliente_NumExterior" placeholder="" onchange="ValidaInput('Cliente_NumExterior')">
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="Cliente_NumInterior"><strong> Interior: </strong> </label>
                                        <input type="text" class="form-control" id="Cliente_NumInterior" placeholder="" onchange="ValidaInput('Cliente_NumInterior')">
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="exampleFormControlSelect1"><strong>* Colonia: </strong></label>
                                        <select class="form-control" id="Select_Colonia" onchange="callCP(this.value)">
                                            <option value="0">Seleccionar </option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="exampleFormControlSelect1"><strong>* Estado: </strong></label>
                                        <select class="form-control" id="Select_DEstado" onchange="callEstados(this.value)">
                                            <option value="0">Seleccionar </option>
                                            @foreach (var item1 in ViewBag.CatEstados)
                                            {
                                                <option value="@item1.Id">@item1.Estado</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="exampleFormControlSelect1"><strong>* Ciudad o municipio: </strong></label>
                                        <select class="form-control" id="Select_Delegacion_Municipio" onchange="callColonias(this.value)">
                                            <option value="0">Seleccionar </option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="Cliente_EntreCalles"><strong> ¿ Entre que calles está el domicilio ? </strong> </label>
                                        <input type="text" class="form-control" id="Cliente_EntreCalles" placeholder="" onchange="ValidaInput('Cliente_EntreCalles')">
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label for="Cliente_Cerca"><strong> Referencias, ¿ Qué se encuentra cerca ? </strong> </label>
                                        <input type="text" class="form-control" id="Cliente_Cerca" placeholder="" onchange="ValidaInput('Cliente_Cerca')">
                                    </div>
                                </div>
                                <div class="col-md-1">
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <div class="form-check">
                                            <input type="checkbox" class="form-check-input" id="checkboxRecibirPedido">
                                            <label class="texto" style="font-size: 16px; color: #666666; font-weight: bold;" for="checkboxRecibirPedido">Quiero que alguien más pueda recibir mi pedido</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6" id="RV_Nombre" style="display: none;">
                                    <div class="form-group">
                                        <label for="Cliente_Recibir_Nombre"><strong>* Nombre (s): </strong></label>
                                        <input type="text" class="form-control" id="Cliente_Recibir_Nombre" placeholder="" onchange="ValidaInput('Cliente_Recibir_Nombre')">
                                    </div>
                                </div>
                                <div class="col-md-6" id="RV_Apellidos" style="display: none;">
                                    <div class="form-group">
                                        <label for="Cliente_Recibir_Apellidos"><strong>* Apellidos: </strong></label>
                                        <input type="text" class="form-control" id="Cliente_Recibir_Apellidos" placeholder="" onchange="ValidaInput('Cliente_Recibir_Apellidos')">
                                    </div>
                                </div>
                                <div class="col-md-6" id="RV_Telefono" style="display: none;">
                                    <div class="form-group">
                                        <label for="Cliente_Recibir_Telefono"><strong>* Teléfono: </strong></label>
                                        <input type="text" class="form-control input-number" id="Cliente_Recibir_Telefono" maxlength="12" placeholder="" onchange="ValidaInput('Cliente_Recibir_Telefono')">
                                    </div>
                                </div>
                                <div class="col-md-6" id="RV_TipoTelefono" style="display: none;">
                                    <br />
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="RadiosClienteTipoTelefonoRecibe" id="exampleRadios1" value="Fijo" checked>
                                        <label class="form-check-label" for="exampleRadios1">
                                            Fijo
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="radio" name="RadiosClienteTipoTelefonoRecibe" id="exampleRadios2" value="Celular">
                                        <label class="form-check-label" for="exampleRadios2">
                                            Celular
                                        </label>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <div class="form-check">
                                            <input type="checkbox" class="form-check-input" id="checkboxRecibirPublicidad">
                                            <label class="texto" style="font-size: 16px; color: #666666; font-weight: bold;" for="checkboxRecibirPublicidad">Quiero recibir publicidad y/ó propaganda </label>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="BtnCompra col-md-4">
                                <a href="@Url.Action("Carro", "Store")" class="flex-c-m stext-101 cl0 size-116 bg1 bor14 hov-btn1 p-lr-15 trans-04 pointer" id="Btn_NuevoDomicilio">
                                    <i class="fa fa-arrow-left" aria-hidden="true"></i> &nbsp; Ir al carrito
                                </a>
                            </div>
                            <div class="BtnCompraEspacing col-md-4">
                            </div>
                            <div class="BtnCompra col-md-4">
                                <button class="flex-c-m stext-101 cl0 size-116 bg3 bor14 hov-btn3 p-lr-15 trans-04 pointer formatopago" id="Btn_FormatoPago">
                                    Elegir formato de pago &nbsp; <i class="fa fa-arrow-right" aria-hidden="true"></i>
                                </button>
                            </div>
                        </div>

                    </div>
                </div>

                <div class="col-sm-10 col-lg-7 col-xl-5 m-lr-auto m-b-50">
                    <div class="bor10 p-lr-20 p-t-30 p-b-20 m-l-63 m-r-20 m-lr-0-xl p-lr-15-sm" style="background-color: #f7f8f9">
                        <h4 class="mtext-109 cl2 p-b-30 text-center">
                            Resumen de compra
                        </h4>

                        <div class="flex-w  bor12 p-b-13" style="background-color: #ffffff; padding: 20px;">
                            <div class="size-208">
                                <span class="stext-110 cl2">
                                    Subtotal:
                                </span>
                            </div>

                            <div class="size-209">
                                <span class="mtext-110 cl2 text-right" id="SubTotal_">
                                    $@ViewBag.Total
                                </span>
                            </div>
                        </div>

                        <div class="flex-w  bor12 p-b-13" style="background-color: #ffffff; padding: 20px;">
                            <div class="size-208">
                                <span class="stext-110 cl2">
                                    Envio:
                                </span>
                            </div>

                            <div class="size-209 p-r-18 p-r-0-sm w-full-ssm">
                                <p class="mtext-110 cl2" id="TotalEnvio_">
                                    $0.00
                                </p>
                            </div>
                        </div>

                        <div class="flex-w p-t-27 p-b-33" style="background-color: #ffffff; padding: 20px;">
                            <div class="size-208">
                                <span class="mtext-101 cl2">
                                    Total:
                                </span>
                            </div>

                            <div class="size-209 p-t-1">
                                <span class="mtext-110 cl2" id="Total_">
                                    $@ViewBag.Total
                                </span>
                            </div>
                        </div>

                        <br />
                        <h5 class="mtext-109 cl2 p-b-5 text-center">Entrega a domicilio</h5>
                        <div class="flex-w bor12 p-t-15 p-b-30">
                            <div class=" p-r-18 p-r-0-sm w-full-ssm">
                                <p class="stext-111  p-t-2">
                                    *La fecha de entrega puede cambiar por la disponibilidad del producto, tu ubicación o la forma de pago.
                                </p>
                            </div>
                        </div>

                        <br />
                        <div class="flex-w bor12 p-t-15 p-b-30">
                            <div class=" p-r-18 p-r-0-sm w-full-ssm">
                                <table>
                                    @{

                                        foreach (var Arti in ViewBag.ListaArticulos)
                                        {
                                            <tr>
                                                <td>
                                                    <div class="how-itemcart1">
                                                        <img src="https://@Arti.Imagen" alt="IMG">
                                                    </div>
                                                </td>
                                                <td>
                                                    <p>
                                                        <strong>@Arti.Nombre</strong>
                                                    </p>
                                                    <label style="margin-bottom: 0rem; font-size: 12px; text-transform: lowercase; ">Categoria: <strong style="">@Arti.Clasificacion</strong></label>
                                                    Cantidad : <strong>@Arti.Item</strong>
                                                    <strong><label id="Total_@Arti.Id">Total: @Arti.TotalText</label></strong>
                                                    <br />
                                                </td>
                                            </tr>
                                        }

                                    }
                                </table>
                            </div>
                        </div>
                        <br />
                        <div class="flex-w text-right" style="font-size:25px">
                            <div>
                                <strong>
                                    <span class="">
                                        Total:
                                    </span>
                                    <span class="" id="Total_">
                                        $ @ViewBag.Total
                                    </span>
                                </strong>
                            </div>
                        </div>
                        <br />
                    </div>
                </div>
            </div>
        </div>

    </div>


    <!-- Footer -->
    <footer class="bg3 p-t-75 p-b-32">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-lg-3 p-b-50">
                    <h4 class="stext-301 cl0 p-b-10">
                        ¿ Quiénes somos ?
                    </h4>
                    <p style="color:#ffffff; font-size: 13px;">
                        @*Empresa 100% mexicana, con más de 30 años de experiencia ayudando a empresas nacionales e internacionales.*@
                    </p>
                    <hr />
                    <ul class="footer-links">
                        <li>
                            <a href="#">
                                <i class="fa fa-map-marker"></i>
                                ...
                            </a>
                        </li>
                        <li><a href="#"><i class="fa fa-phone"></i> @*55 2909 7024 / 55 3566 4390*@</a></li>
                        <li><a href="#"><i class="fa fa-envelope-o"></i>@*sig_contacto@serviciosinmobiliariosg.com*@</a></li>
                    </ul>

                    <div class="p-t-27">
                        <a href="#" class="fs-18 cl7 hov-cl1 trans-04 m-r-16">
                            <i class="fa fa-facebook"></i>
                        </a>

                        <a href="#" class="fs-18 cl7 hov-cl1 trans-04 m-r-16">
                            <i class="fa fa-instagram"></i>
                        </a>

                        <a href="#" class="fs-18 cl7 hov-cl1 trans-04 m-r-16">
                            <i class="fa fa-pinterest-p"></i>
                        </a>
                    </div>
                </div>

                <div class="col-sm-9 col-lg-3 p-b-50">
                    <h4 class="stext-301 cl0 p-b-10">
                        Categories
                    </h4>

                    <ul class="footer-links">
                        @*<li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Accesorios De Alimentación</a></li>
                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Accesorios De Computadoras</a></li>
                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Bocinas </a></li>
                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Cámara</a></li>
                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Computadora </a></li>
                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Dispositivos Para Redes </a></li>
                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Oficina Y Papelería </a></li>
                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Radios</a></li>
                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Seguridad</a></li>
                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Software </a></li>
                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Tabletas</a></li>*@
                    </ul>
                </div>

                <div class="col-sm-5 col-lg-2 p-b-50">
