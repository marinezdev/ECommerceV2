                };

                $.ajax({
                    type: "POST",
                    url: "@Url.Action("CP_Busqueda", "Compra_Direccion")",
                    data: JSON.stringify(jsonObject),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: resultadoBusqueda,
                    error: errores
                });
