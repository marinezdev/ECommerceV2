            if ($('#checkboxRecibirPedido').prop('checked')) {
                if ($('#Cliente_Recibir_Nombre').val().length > 0) {
                    if ($('#Cliente_Recibir_Apellidos').val().length > 0) {
                        if ($('#Cliente_Recibir_Telefono').val().length > 0) {
                            RecibirPedido = true;
                        }
                    }
                }
            } else {
                RecibirPedido = true;
            }

            if (Contacto) {
                if (direecion) {
                    if (RecibirPedido) {
                        resultado = true;
                    }
                }
            }
            
            return resultado;
        }

