                var textoMayusculas = textoActual.toUpperCase();

                // Verificar si el valor ha cambiado antes de actualizar el input
                // Esto evita que se cree un ciclo infinito en algunos navegadores
                if (textoActual !== textoMayusculas) {
                    // Actualizar el valor del input con el texto en mayúsculas y sin números
                    $(this).val(textoMayusculas);
                }
            });
        });

        function ValidaInput(name) {
            if ($('#' + name).val().length > 0) {
                $('#' + name).css("border", "1px solid rgb(222 222 222)");
            } else {
                $('#' + name).css("border", "1px solid #ff0049");
            }
        }

        function ValidaSelect(name) {
            if ($('#' + name + ' option:selected').val() > 0) {
                $('#' + name).css("border", "1px solid rgb(222 222 222)");
            } else {
                $('#' + name).css("border", "1px solid #ff0049");
            }
        }

        $('#BtnCancelarNuevoDomicilio').click(function () {
            $("#Select_Direccion option[value='0']").attr("selected", true);

            $("#div_NuevaDireccionEntrega").css("display", "none");
            $("#ND_SDomicilio").show();
            $("#ND_BtnDomicilio").show();
            $("#Btn_FormatoPago3").css("display", "none");

        });

        $('#BtnCancelarEditarDomicilio').click(function () {

            $("#div_EditarDireccionEntrega").css("display", "none");
            $("#ND_SDomicilio").show();
            $("#ND_BtnDomicilio").show();
            $("#div_Direcciones").show();
            $("#Btn_EditarDomicilio").show();
            $("#BtnActulizarDireccion").css("display", "none");
            $("#Btn_FormatoPago2").show();
            
        });

        $('#Btn_AgregarDomicilio').click(function () {
            $("#ND_SDomicilio").css("display", "none");
            $("#ND_BtnDomicilio").css("display", "none");
            $("#div_Direcciones").css("display", "none");
            $("#Btn_EditarDomicilio").css("display", "none");
            $("#Btn_FormatoPago2").css("display", "none");
            $("#div_NuevaDireccionEntrega").show();
            $("#Btn_FormatoPago3").show();
        });
    </script>
</body>
</html>
