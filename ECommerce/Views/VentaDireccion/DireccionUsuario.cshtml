    <script>

        function ActualizarDireccion(Id) {
            if (isValidForm3()) {
                var CatTipoTelefono = new Object();
                CatTipoTelefono.Id = 0;

                var Telefono = new Object();
                Telefono.CatTipoTelefono = CatTipoTelefono;

                var Persona = new Object();
                if ($('#Editar_checkboxRecibirPublicidad').prop('checked')) {
                    Persona.Publicidad = 1;
                } else {
                    Persona.Publicidad = 0;
                }

                var CatColonias = new Object();
                CatColonias.Id = $("#Editar_Select_Colonia option:selected").val();

                var PersonaDireccion = new Object();
                PersonaDireccion.Id = Id;
                PersonaDireccion.Nombre = $('#Editar_Cliente_NombreDireccion').val();
                PersonaDireccion.CatColonias = CatColonias;
                PersonaDireccion.NumExterior = $('#Editar_Cliente_NumExterior').val();
                PersonaDireccion.NumInterior = $('#Editar_Cliente_NumInterior').val();
                PersonaDireccion.Calle = $('#Editar_Cliente_Calle').val();
                PersonaDireccion.EntreCalles = $('#Editar_Cliente_EntreCalles').val();
                PersonaDireccion.Referencias = $('#Editar_Cliente_Cerca').val();

                if ($('#Editar_checkboxRecibirPedido').prop('checked')) {
                    PersonaDireccion.RecibirPedido = 1;
                    PersonaDireccion.RecibirNombre = $('#Editar_Cliente_Recibir_Nombre').val();
                    PersonaDireccion.RecibirApellidos = $('#Editar_Cliente_Recibir_Apellidos').val();
                    PersonaDireccion.RecibirTelefono = $('#Editar_Cliente_Recibir_Telefono').val()
                    PersonaDireccion.RecibirTipoTelefono = $('input:radio[name=Editar_RadiosClienteTipoTelefonoRecibe]:checked').val();
                } else {
                    PersonaDireccion.RecibirPedido = 0;
                }

                var UsuarioInformacion = new Object();
                UsuarioInformacion.Telefono = Telefono;
                UsuarioInformacion.Persona = Persona;
                UsuarioInformacion.PersonaDireccion = PersonaDireccion;

                $.ajax({
