
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DireccionInvitado</title>
    <link rel="shortcut icon" href="~/assets/images/icoCSB.ico">
    <link rel="stylesheet" type="text/css" href="~/vendor/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/fonts/iconic/css/material-design-iconic-font.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/Content/css/main.css">
    <link rel="stylesheet" type="text/css" href="https://common.olemiss.edu/_js/sweet-alert/sweet-alert.css">
    <style>
        .wrap-menu-desktop {
            position: fixed;
            z-index: 101;
            /* background-color: #e9ecefc2; */
            background-color: #ffffffe0;
            width: 100%;
            height: 84px;
            top: 0px;
            left: 0px;
            -webkit-transition: height 0.3s, background-color 0.3s;
            -o-transition: height 0.3s, background-color 0.3s;
            -moz-transition: height 0.3s, background-color 0.3s;
            transition: height 0.3s, background-color 0.3s;
        }

        .how-itemcart1 {
            width: 130px;
            position: relative;
            margin-right: 20px;
            cursor: pointer;
        }

        .bor14 {
            border-radius: 10px;
        }

        .how-itemcart1::after {
            content: '';
            font-family: Linearicons-Free;
            font-size: 16px;
            display: -webkit-box;
            display: -webkit-flex;
            display: -moz-box;
            display: -ms-flexbox;
            display: flex;
            justify-content: center;
            align-items: center;
            position: absolute;
            width: 100%;
            height: 100%;
            top: 0;
            left: 0;
            background-color: rgb(0 0 0 / 0%);
            color: #fff;
            transition: all 0.3s;
            -webkit-transition: all 0.3s;
            -o-transition: all 0.3s;
            -moz-transition: all 0.3s;
            opacity: 0;
        }

        /*.form-control {
                display: block;
                width: 100%;
                padding: 0.5rem 0.75rem;
                font-size: 1rem;
                line-height: 1.25;
                color: #495057;
                background-color: #fff;
                background-image: none;
                background-clip: padding-box;
                border: 1px solid rgba(0,0,0,.15);
                border-radius: 0.25rem;
                transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s;
                height:42px;
            }*/
        .bg1 {
            background-color: #b0ce68;
        }

        .hov-btn1:hover {
            border-color: #b0ce68;
            background-color: #77a4b5;
            color: #fff;
        }

        .footer-links li i {
            margin-right: 15px;
            color: #b0ce68;
            width: 14px;
            text-align: center;
        }

        @@media (max-width: 991px) {
            .estetic {
                display: none;
            }

            .estetic1 {
                display: block !important;
            }
        }


        @@media (max-width: 767px) {
            .form-check {
                position: relative;
                display: block;
                margin-bottom: 0.5rem;
                padding-left: 4%;
            }

            .BtnCompra {
                width: 50% !important;
            }

            .BtnCompraEspacing {
                display: none;
            }
        }

        @@media (max-width: 538px) {
            .form-check {
                position: relative;
                display: block;
                margin-bottom: 0.5rem;
                padding-left: 10%;
            }

            .texto {
                font-size: 15px !important;
            }
        }


        @@media (max-width: 465px) {

            .formatopago {
                font-size: 14px;
            }
        }

        .bor15 {
            border-left: 1px solid #e6e6e6;
            border-right: 1px solid #e6e6e6;
            border-bottom: 1px solid #e6e6e6;
            box-shadow: 0 1px 20px 0 rgb(69 90 100 / 23%);
        }

        .bor10 {
            border: 1px solid #e6e6e6;
            box-shadow: 0 1px 20px 0 rgb(69 90 100 / 23%);
        }
    </style>
</head>
<body>
    <!-- Header -->
    <header>
        <!-- Header desktop -->
        <div class="container-menu-desktop">

            <div class="wrap-menu-desktop">
                <nav class="limiter-menu-desktop container">

                    <!-- Logo desktop -->
                    <a href="#" class="logo">
                        <img src="~/images/icons/CSB_Mexico.png" />
                    </a>

                    <!-- Menu desktop -->
                    <div class="menu-desktop">
                        <ul class="main-menu">
                        </ul>
                    </div>

                    <!-- Icon header -->
                    <div class="wrap-icon-header flex-w flex-r-m">
                        <a href="@Url.Action("Index", "CarroElectronico")" class="flex-c-m cl0 size-103 bg1 bor1 hov-btn2 p-lr-15 trans-04">
                            <i class="fa fa-reply" aria-hidden="true"></i>  &nbsp;&nbsp;  volver al carrito
                        </a>
                    </div>
                </nav>
            </div>
        </div>
    </header>



    <div class="top-content">
        <p class="estetic">
            <br />
            <br />
            <br />
            <br />
            <br />
        </p>
        <p class="estetic1" style="display:none;">
            <br />
        </p>
        <div class="container">
            <div class="row">
                <div class="col-lg-10 col-xl-7 m-lr-auto m-b-50">
                    <div class="m-l-25 m-r--38 m-lr-0-xl">
                        @{

                            if (ViewBag.ListaDireciones != null)
                            {
                                <div class=" bor15 p-t-18 p-b-15 p-lr-40 p-lr-15-sm" style="border-top: 1px solid #e6e6e6; border-radius: 5px;">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <h5><strong style=" color: #222222;">1.- DIRECCIÓN DE ENVÍO  </strong> </h5>
                                            <hr />
                                            <strong><label style="font-size: 16px; color: #222222;">Información del cliente</label></strong>
                                            <p>Te enviaremos una confirmación del pedido a tu correo electrónico </p>
                                            <br />
                                            <strong><label style="font-size: 16px; color: #222222;">Correo electrónico:</label> </strong>
                                            <p>@ViewBag.UsuarioInformacion.Email.email </p>
                                            <br />
                                            <strong><label style="font-size: 16px; color: #222222;">Nombre:</label> </strong>
                                            <p>@ViewBag.UsuarioInformacion.Persona.Nombre @ViewBag.UsuarioInformacion.Persona.ApellidoPaterno @ViewBag.UsuarioInformacion.Persona.ApellidoMaterno</p>
                                            @if (ViewBag.UsuarioInformacion.Telefono != null)
                                            {
                                                <br />
                                                <strong><label style="font-size: 16px; color: #222222;">Teléfono :</label> </strong>
                                                <p>@ViewBag.UsuarioInformacion.Telefono.telefono - <strong>@ViewBag.UsuarioInformacion.Telefono.CatTipoTelefono.Nombre</strong></p>
                                            }
                                            <br />
                                        </div>
                                        <div class="col-md-6" id="ND_SDomicilio">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1"><strong>* Elegir domicilio registrado: </strong></label>
                                                <select class="form-control" id="Select_Direccion" onchange="callDireccion(this.value)">
                                                    <option value="0">SELECCIONAR </option>
                                                    @foreach (var item1 in ViewBag.ListaDireciones)
                                                    {
                                                        <option value="@item1.Id">@item1.Nombre</option>
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-4" id="ND_BtnDomicilio">
                                            <br />
                                            <button type="button" class="flex-c-m stext-101 cl0 size-116 bg3 bor14 hov-btn3 p-lr-15 trans-04 pointer" id="Btn_AgregarDomicilio">
                                                Agregar otra dirección
                                            </button>
                                        </div>
                                        <div class="col-md-12" id="div_Direcciones" style="display: none;">
                                            <br />
                                            <p>Dirección de envío:</p>
                                            <strong><label id="Label_Direccion"></label></strong>
                                            <br />
                                            <label>Recibirá el pedido: <strong><span id="LabelRecibe_Nombre"></span></strong></label>
                                        </div>
                                        <div class="col-md-12" style="font-size: 14px; display: none;" id="Btn_EditarDomicilio">

                                        </div>
                                    </div>
                                </div>
                                <br />
                                <div class=" bor15 p-t-18 p-b-15 p-lr-40 p-lr-15-sm" id="div_NuevaDireccionEntrega" style="border-top: 1px solid #e6e6e6; border-radius: 5px; display: none;">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <h5><strong style=" color: #222222;">2.- NUEVO DOMICILIO DE ENTREGA  </strong> </h5>
                                            <hr />
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="CP"><strong>* Nombre de la nueva dirección : </strong></label>
                                                <input type="text" class="form-control input-mayusculas" name="Cliente_NombreDireccion" id="Cliente_NombreDireccion" placeholder="" onchange="ValidaInput('Cliente_NombreDireccion')">
                                            </div>
                                        </div>
                                        <div class="col-md-6">

                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="CP"><strong>* Código postal: </strong></label>
                                                <input type="text" class="form-control input-number" name="CP" id="CP" maxlength="6" placeholder="Código postal" onchange="CPBuscar(this.value)">
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label for="Cliente_Calle"><strong>* Calle: </strong></label>
                                                <input type="text" class="form-control input-mayusculas" id="Cliente_Calle" placeholder="" onchange="ValidaInput('Cliente_Calle')">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="Cliente_NumExterior"><strong>* Número exterior: </strong></label>
                                                <input type="text" class="form-control input-mayusculas" id="Cliente_NumExterior" placeholder="" onchange="ValidaInput('Cliente_NumExterior')">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="Cliente_NumInterior"><strong> Interior: </strong> </label>
                                                <input type="text" class="form-control input-mayusculas" id="Cliente_NumInterior" placeholder="">
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1"><strong>* Colonia: </strong></label>
                                                <select class="form-control" id="Select_Colonia" onchange="callCP(this.value)">
                                                    <option value="0">Seleccionar </option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1"><strong>* Estado: </strong></label>
                                                <select class="form-control" id="Select_DEstado" onchange="callEstados(this.value)">
                                                    <option value="0">Seleccionar </option>
                                                    @foreach (var item1 in ViewBag.Estados)
                                                    {
                                                        <option value="@item1.Id">@item1.Estado</option>
                                                    }
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label for="exampleFormControlSelect1"><strong>* Ciudad o municipio: </strong></label>
                                                <select class="form-control" id="Select_Delegacion_Municipio" onchange="callColonias(this.value)">
                                                    <option value="0">Seleccionar </option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label for="Cliente_EntreCalles"><strong> ¿ Entre que calles está el domicilio ? </strong> </label>
                                                <input type="text" class="form-control input-mayusculas" id="Cliente_EntreCalles" placeholder="" onchange="ValidaInput('Cliente_EntreCalles')">
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <label for="Cliente_Cerca"><strong> Referencias, ¿ Qué se encuentra cerca ? </strong> </label>
                                                <input type="text" class="form-control input-mayusculas" id="Cliente_Cerca" placeholder="" onchange="ValidaInput('Cliente_Cerca')">
                                            </div>
                                        </div>
                                        <div class="col-md-1">
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <div class="form-check">
                                                    <input type="checkbox" class="form-check-input" id="checkboxRecibirPedido">
                                                    <label style="font-size: 16px; color: #666666; font-weight: bold;" for="checkboxRecibirPedido">Quiero que alguien más pueda recibir mi pedido</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6" id="RV_Nombre" style="display: none;">
                                            <div class="form-group">
                                                <label for="Cliente_Recibir_Nombre"><strong>* Nombre (s): </strong></label>
                                                <input type="text" class="form-control input-mayusculas" id="Cliente_Recibir_Nombre" placeholder="" onchange="ValidaInput('Cliente_Recibir_Nombre')">
                                            </div>
                                        </div>
                                        <div class="col-md-6" id="RV_Apellidos" style="display: none;">
                                            <div class="form-group">
                                                <label for="Cliente_Recibir_Apellidos"><strong>* Apellidos: </strong></label>
                                                <input type="text" class="form-control input-mayusculas" id="Cliente_Recibir_Apellidos" placeholder="" onchange="ValidaInput('Cliente_Recibir_Apellidos')">
                                            </div>
                                        </div>
                                        <div class="col-md-6" id="RV_Telefono" style="display: none;">
                                            <div class="form-group">
                                                <label for="Cliente_Recibir_Telefono"><strong>* Teléfono: </strong></label>
                                                <input type="text" class="form-control input-number" id="Cliente_Recibir_Telefono" maxlength="12" placeholder="" onchange="ValidaInput('Cliente_Recibir_Telefono')">
                                            </div>
                                        </div>
                                        <div class="col-md-6" id="RV_TipoTelefono" style="display: none;">
                                            <br />
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" name="RadiosClienteTipoTelefonoRecibe" id="exampleRadios1" value="1" checked>
                                                <label class="form-check-label" for="exampleRadios1">
                                                    Fijo
                                                </label>
                                            </div>
                                            <div class="form-check">
                                                <input class="form-check-input" type="radio" name="RadiosClienteTipoTelefonoRecibe" id="exampleRadios2" value="2">
                                                <label class="form-check-label" for="exampleRadios2">
                                                    Celular
                                                </label>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="form-group">
                                                <div class="form-check">
                                                    <input type="checkbox" class="form-check-input" id="checkboxRecibirPublicidad">
                                                    <label style="font-size: 16px; color: #666666; font-weight: bold;" for="checkboxRecibirPublicidad">Quiero recibir publicidad y/ó propaganda </label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-8">
                                        </div>
                                        <div class="col-md-4">
                                            <button type="button" class="flex-c-m stext-101 cl0 size-116 bg3 bor14 hov-btn3 p-lr-15 trans-04 pointer" id="BtnCancelarNuevoDomicilio">
                                                Cancelar
                                            </button>
                                        </div>
                                    </div>
                                </div>

                                <br />
                                <form id="div_EditarDireccionEntrega" style="display: none;">
                                    <div class=" bor15 p-t-18 p-b-15 p-lr-40 p-lr-15-sm" id="" style="border-top: 1px solid #e6e6e6; border-radius: 5px;">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <h5><strong style=" color: #222222;">2.- EDITAR DOMICILIO DE ENTREGA  </strong> </h5>
                                                <hr />
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="Editar_Cliente_NombreDireccion"><strong>* Nombre de la nueva dirección : </strong></label>
                                                    <input type="text" class="form-control input-mayusculas" name="Editar_Cliente_NombreDireccion" id="Editar_Cliente_NombreDireccion" onchange="ValidaInput('Editar_Cliente_NombreDireccion')">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="CP"><strong>* Código postal: </strong></label>
                                                    <input type="text" class="form-control input-number" name="Editar_CP" id="Editar_CP" maxlength="6" placeholder="Código postal" onchange="Editar_CPBuscar(this.value)">
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label for="Cliente_Calle"><strong>* Calle: </strong></label>
                                                    <input type="text" class="form-control input-mayusculas" id="Editar_Cliente_Calle" placeholder="" onchange="ValidaInput('Editar_Cliente_Calle')">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="Editar_Cliente_NumExterior"><strong>* Número exterior: </strong></label>
                                                    <input type="text" class="form-control input-mayusculas" id="Editar_Cliente_NumExterior" placeholder="" onchange="ValidaInput('Editar_Cliente_NumExterior')">
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="Cliente_NumInterior"><strong> Interior: </strong> </label>
                                                    <input type="text" class="form-control input-mayusculas" id="Editar_Cliente_NumInterior" placeholder="">
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label for="exampleFormControlSelect1"><strong>* Colonia: </strong></label>
                                                    <select class="form-control" id="Editar_Select_Colonia" onchange="Editar_callCP(this.value)">
                                                        <option value="0">Seleccionar </option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="exampleFormControlSelect1"><strong>* Estado: </strong></label>
                                                    <select class="form-control" id="Editar_Select_DEstado" onchange="Editar_callEstados(this.value)">
                                                        <option value="0">Seleccionar </option>
                                                        @foreach (var item1 in ViewBag.Estados)
                                                        {
                                                            <option value="@item1.Id">@item1.Estado</option>
                                                        }
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label for="exampleFormControlSelect1"><strong>* Ciudad o municipio: </strong></label>
                                                    <select class="form-control" id="Editar_Select_Delegacion_Municipio" onchange="Editar_callColonias(this.value)">
                                                        <option value="0">Seleccionar </option>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label for="Cliente_EntreCalles"><strong> ¿ Entre que calles está el domicilio ? </strong> </label>
                                                    <input type="text" class="form-control input-mayusculas" id="Editar_Cliente_EntreCalles" placeholder="" onchange="ValidaInput('Editar_Cliente_EntreCalles')">
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label for="Cliente_Cerca"><strong> Referencias, ¿ Qué se encuentra cerca ? </strong> </label>
                                                    <input type="text" class="form-control input-mayusculas" id="Editar_Cliente_Cerca" placeholder="" onchange="ValidaInput('Editar_Cliente_Cerca')">
                                                </div>
                                            </div>
                                            <div class="col-md-1">
                                            </div>
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <div class="form-check">
                                                        <input type="checkbox" class="form-check-input" id="Editar_checkboxRecibirPedido">
                                                        <label style="font-size: 16px; color: #666666; font-weight: bold;" for="Editar_checkboxRecibirPedido">Quiero que alguien más pueda recibir mi pedido</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6" id="Editar_RV_Nombre" style="display: none;">
                                                <div class="form-group">
                                                    <label for="Editar_Cliente_Recibir_Nombre"><strong>* Nombre (s): </strong></label>
                                                    <input type="text" class="form-control input-mayusculas" id="Editar_Cliente_Recibir_Nombre" placeholder="" onchange="ValidaInput('Editar_Cliente_Recibir_Nombre')">
                                                </div>
                                            </div>
                                            <div class="col-md-6" id="Editar_RV_Apellidos" style="display: none;">
                                                <div class="form-group">
                                                    <label for="Editar_Cliente_Recibir_Apellidos"><strong>* Apellidos: </strong></label>
                                                    <input type="text" class="form-control input-mayusculas" id="Editar_Cliente_Recibir_Apellidos" placeholder="" onchange="ValidaInput('Editar_Cliente_Recibir_Apellidos')">
                                                </div>
                                            </div>
                                            <div class="col-md-6" id="Editar_RV_Telefono" style="display: none;">
                                                <div class="form-group">
                                                    <label for="Editar_Cliente_Recibir_Telefono"><strong>* Teléfono: </strong></label>
                                                    <input type="text" class="form-control input-number" id="Editar_Cliente_Recibir_Telefono" maxlength="12" placeholder="" onchange="ValidaInput('Editar_Cliente_Recibir_Telefono')">
                                                </div>
                                            </div>
                                            <div class="col-md-6" id="Editar_RV_TipoTelefono" style="display: none;">
                                                <br />
                                                <div class="form-check">
                                                    <input class="form-check-input" type="radio" name="Editar_RadiosClienteTipoTelefonoRecibe" id="Editar_RadiosClienteTipoTelefonoRecibe" value="1" checked>
                                                    <label class="form-check-label" for="Editar_RadiosClienteTipoTelefonoRecibe">
                                                        Fijo
                                                    </label>
                                                </div>
                                                <div class="form-check">
                                                    <input class="form-check-input" type="radio" name="Editar_RadiosClienteTipoTelefonoRecibe" id="Editar_RadiosClienteTipoTelefonoRecibe2" value="2">
                                                    <label class="form-check-label" for="Editar_RadiosClienteTipoTelefonoRecibe2">
                                                        Celular
                                                    </label>
                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <div class="form-check">
                                                        <input type="checkbox" class="form-check-input" id="Editar_checkboxRecibirPublicidad">
                                                        <label style="font-size: 16px; color: #666666; font-weight: bold;" for="Editar_checkboxRecibirPublicidad">Quiero recibir publicidad y/ó propaganda </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-8">
                                            </div>
                                            <div class="col-md-4">
                                                <button type="button" class="flex-c-m stext-101 cl0 size-116 bg3 bor14 hov-btn3 p-lr-15 trans-04 pointer" id="BtnCancelarEditarDomicilio">
                                                    Cancelar
                                                </button>
